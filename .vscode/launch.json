{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/src/index.js",
      "sourceMaps": true,
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach for debug",
      "restart": false,
      "protocol": "inspector",
      "sourceMaps": true
    }
  ]
}